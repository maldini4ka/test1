{% extends 'base.html.twig' %}

{% block title %}Kinoteatr index{% endblock %}

{% block body %}
    <h1>Kinoteatr index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Info</th>
                <th>Address</th>
                <th>Url</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for kinoteatr in kinoteatrs %}
            <tr>
                <td>{{ kinoteatr.id }}</td>
                <td>{{ kinoteatr.info }}</td>
                <td>{{ kinoteatr.address }}</td>
                <td>{{ kinoteatr.url }}</td>
                <td>
                    <a href="{{ path('kinoteatr_show', {'id': kinoteatr.id}) }}">show</a>
                    <a href="{{ path('kinoteatr_edit', {'id': kinoteatr.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('kinoteatr_new') }}">Create new</a>
{% endblock %}


